<link rel="stylesheet" href="/stylesheets/css/bootstrap.min.css">
<div style="background-image:url('./images/0.jpg');
    background-attachment: fixed;">
<center><h1 style="color:#f1f1f1">Your Videos</h1></center>
<form action="/trending" method="post">
	<button type="Submit" class="btn btn-success">Trending Videos</button>
</form>
{{#each youtube}}
  <div class="container">
  	<h3 style="color:#888">Title: </h3><h4 style="color:#f1f1f1"><u>{{this.title}}</u></h4>
  	<h3 style="color:#888">Video: </h3>
  	<div class="container-sm"><iframe width="100%" height="400" src="{{this.clink}}"></iframe></div>
  </div>
  <br><hr>
{{/each}}
</div>
